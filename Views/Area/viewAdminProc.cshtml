@model viewAdminProcForPerformer

@if(Model!=null)
    {
       <ol id="adminProcList">
            @foreach (adminProcModel proc in Model.adminProcList)
            {
                <li value="@proc.head.Number">
                    <span>@proc.head.Name</span>
                    <ol style="display:none;">
                        @foreach (sectionAdminProc sec in proc.section)
                        {
                            <li value="@sec.section.Number">
                                <span>@sec.section.Name</span>
                                <ol style="display:none;">
                                    @foreach (docRegModel doc in sec.docReg)
                                    {
                                        <li value="@doc.Num">
                                            <label style="display:inline-block"><input type="checkbox" checked="@((Model.docReg!=null&&Model.docReg.Contains(doc.RegID))||false)" data-id="@doc.RegID" name="docReg"/> Исполнитель </label>
                                            <label style="display:inline-block"><input type="checkbox" checked="@((Model.docRes!=null&&Model.docRes.Contains(doc.RegID))||false)" data-id="@doc.RegID" name="docRes"/> Ответственный </label>
                                            <label style="display:inline-block"><input type="checkbox" checked="@((Model.docAcc!=null&&Model.docAcc.Contains(doc.RegID))||false)" data-id="@doc.RegID" name="docAcc"/> Прием документов </label>
                                             <b>@doc.Number</b> @doc.RegName                                                                                 
                                            <hr />
                                        </li>
                                    }
                                </ol>
                            </li>
                        }
                    </ol>
                </li>
            }
        </ol>
    }